{
  "name": "dogs",
  "version": "1.0.0",
  "description": "Full stack web application using React, Node.js, Koa and Webpack",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node server/server.js",
    "build": "webpack --mode=production",
    "client": "webpack --mode=development",
    "server": "nodemon server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest --watch"
  },
  "author": "Yaokai Dong",
  "dependencies": {
    "@koa/cors": "^3.4.1",
    "@koa/router": "^12.0.0",
    "axios": "^1.0.0",
    "babel-preset-es2015": "^6.24.1",
    "isomorphic-fetch": "^3.0.0",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-static": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.1.2",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.4.0",
    "css-loader": "^6.7.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "nodemon": "^2.0.20",
    "style-loader": "^3.3.1",
    "supertest": "^6.3.0",
    "webpack": "^5.73.0",
